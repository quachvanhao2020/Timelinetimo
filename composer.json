{
    "name": "administrator/timelinetimo",
    "authors": [
        {
            "name": "vanhaohero1998",
            "email": "quachvanhao2020@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Timelinetimo\\": "src/"
        },
        "files": [
            "src/awake.php",
            "src/functions.php"
        ]
    },
    "require": {
        "php": ">=5.3",
        "hao/yphp": "dev-main",
        "laminas/laminas-permissions-acl": "^2.7",
        "laminas/laminas-permissions-rbac": "^3.0",
        "laminas/laminas-authentication": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "doctrine/orm": "^2.8"
    },
    "repositories": [
        {"type": "vcs", "url": "https://github.com/quachvanhao2020/YPHP.git"}
    ],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "laminas-development-mode disable",
        "development-enable": "laminas-development-mode enable",
        "development-status": "laminas-development-mode status"
    }
}
